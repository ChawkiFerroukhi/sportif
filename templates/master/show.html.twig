{% extends 'base.html.twig' %}

{% block title %}Master{% endblock %}

{% block body %}
    <h1>Master</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ master.id }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ master.createdat ? master.createdat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ master.updatedat ? master.updatedat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ master.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ master.prenom }}</td>
            </tr>
            <tr>
                <th>NumTel</th>
                <td>{{ master.numTel }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ master.cin }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_master_index') }}">back to list</a>

    <a href="{{ path('app_master_edit', {'id': master.id}) }}">edit</a>

    {{ include('master/_delete_form.html.twig') }}
{% endblock %}
