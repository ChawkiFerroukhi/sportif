{% extends 'base.html.twig' %}

{% block title %}Cycle{% endblock %}

{% block body %}
    <h1>Cycle</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cycle.id }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ cycle.createdat ? cycle.createdat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ cycle.updatedat ? cycle.updatedat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ cycle.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cycle_index') }}">back to list</a>

    <a href="{{ path('app_cycle_edit', {'id': cycle.id}) }}">edit</a>

    {{ include('cycle/_delete_form.html.twig') }}
{% endblock %}
