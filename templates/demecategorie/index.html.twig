{% extends 'base.html.twig' %}

{% block title %}Demecategorie index{% endblock %}

{% block body %}
    <h1>Demecategorie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for demecategorie in demecategories %}
            <tr>
                <td>{{ demecategorie.id }}</td>
                <td>{{ demecategorie.createdat ? demecategorie.createdat|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ demecategorie.updatedat ? demecategorie.updatedat|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ demecategorie.nom }}</td>
                <td>
                    <a href="{{ path('app_demecategorie_show', {'id': demecategorie.id}) }}">show</a>
                    <a href="{{ path('app_demecategorie_edit', {'id': demecategorie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_demecategorie_new') }}">Create new</a>
{% endblock %}
